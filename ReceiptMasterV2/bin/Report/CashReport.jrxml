	<subDataset name="CashData" uuid="814ab6b9-d830-416f-bdb3-47ade86a9530">
		<parameter name="FromDateP" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="ToDateP" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[Select
Pe.purchase_entry_id,
Pe.purchase_date,
Pe.invoice_date,
Pe.subtotal,
Pe.total,
(select 'Purchase') as Purchase,
(select '0') as CashReceived,
Pe.paid_amount,
Pay.sales_entry_id,
Pay.purchase_entry_id,
Pay.cash_amt
From rm_payment_modes Pay
INNER JOIN    rm_purchase_entry Pe ON  Pay.purchase_entry_id=Pe.id
WHERE Pe.purchase_date>=$P{FromDateP} AND Pe.purchase_date<=$P{ToDateP}]]>
