	<queryString>
		<![CDATA[select Co.Company_Name,
Co.Address,
Co.officeaddress,
Co.EmaiId,
Co.ContactNo,
Co.altcontactno,
Co.State,
Co.logoPath,
Co.gstin,

sup.supplier_name,
sup.owner_name,
sup.vattinno,
sup.address as supplierAddress,
sup.contactno as suppliercontactNo,
sup.email,
sup.gststate,

prod.productname,
prod.unit,
prod.id,
prod.hsn,
prod.quantity,
prod.buy_price,
prod.sell_price,
prod.category,
prod.subgroup,
prod.info,
prod.gst,
prod.cess,

pr.supplier_id,
pr.purchase_entry_id,
pr.purchase_product_id,
pr.purchase_product_items_id,
pr.return_quantity,
pr.date_created,
pr.status ,
(select((prod.buy_price*pr.return_quantity)*(100/(100+prod.gst))))  as taxable_ammount,


(select (CASE
   when sup.gststate<>'maharashtra'  then (select(prod.gst)) else 0
    END) )as other_state_igst_rate,

 (select (CASE sup.gststate
   when 'maharashtra'  then (select(prod.gst/2)) else 0

END) )as maha_sgst_rate,

(select (CASE sup.gststate
   when 'maharashtra'  then (select(taxable_ammount*(maha_sgst_rate/100))) else 0

END) )as maha_ammount,


(select (CASE
  when  sup.gststate<>  'maharashtra'  then (select(taxable_ammount*(other_state_igst_rate/100))) else 0

END) )as other_ammount,
(select (CASE
  when  sup.gststate<>  'maharashtra'  then (select(taxable_ammount+other_ammount)) else (select(taxable_ammount+maha_ammount+maha_ammount))

END) ) as Goss_Ammount,
(select ROUND(sum(goss_Ammount))) as total_Grand

from company Co,rm_purchase_return pr
INNER JOIN rm_supplier sup ON sup.id=pr.supplier_id
INNER JOIN rm_product prod ON prod.id=pr.purchase_product_items_id ORDER BY pr.id DESC LIMIT 1]]>
	</queryString>
